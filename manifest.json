{
  "manifest_version": 3,
  "name": "Chrome New Tab",
  "description": "Chrome New Tab",
  "homepage_url": "http://ryosuzuki.org",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "chrome_url_overrides": {
    "newtab": "src/index.html"
  },
  "permissions": [],
  "version": "1.0.7",
  "host_permissions": [
    "https://raw.githubusercontent.com/ryosuzuki/chrome-new-tab/*"
  ],
  "content_security_policy": {
    "extension_pages": "default-src 'self'; img-src 'self' https://raw.githubusercontent.com; style-src 'self' 'unsafe-inline'; script-src 'self'; object-src 'none';"
  }
}
